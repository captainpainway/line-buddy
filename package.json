{
  "name": "magic-kingdom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./index.js",
    "watchifybuild": "watchify ./public/js/script.js -o ./public/js/bundle.js -v",
    "watchivywatch": "watchify ./public/js/react.jsx --extension=.jsx -o ./public/js/compiled.js -v",
    "build": "webpack",
    "watch": "webpack -d --watch"
  },
  "browserify": {
    "extension": [
      "jsx"
    ],
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "dependencies": {
    "aframe": "^0.5.0",
    "aframe-mouse-cursor-component": "^0.5.1",
    "aframe-orbit-controls-component-2": "^0.1.7",
    "aframe-react": "^4.2.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browserify": "^14.4.0",
    "cron": "^1.1.0",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "reactify": "^1.1.1",
    "redux": "^3.7.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.2",
    "themeparks": "^4.1.5",
    "watchify": "^3.9.0",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-stage-0": "^6.24.1"
  }
}
